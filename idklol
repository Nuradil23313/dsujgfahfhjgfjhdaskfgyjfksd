Skybox.SkyboxRt = "http://www.roblox.com/asset/?id=11660423541"
Skybox.SkyboxUp = "http://www.roblox.com/asset/?id=11660423541"
game.Lighting.SkyBox:Remove()
wait(0.1)
Skybox.Parent = game.Lighting
while wait(1) do
Skybox.SkyboxBk = "http://www.roblox.com/asset/?id=7148209019"
