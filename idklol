Skybox.SkyboxBk = "http://www.roblox.com/asset/?id=11660423541"
Skybox.SkyboxDn = "http://www.roblox.com/asset/?id=11660423541"
Skybox.SkyboxFt = "http://www.roblox.com/asset/?id=11660423541"
Skybox.SkyboxLf = "http://www.roblox.com/asset/?id=11660423541"
Skybox.SkyboxRt = "http://www.roblox.com/asset/?id=11660423541"
